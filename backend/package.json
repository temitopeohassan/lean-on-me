    "zod": "^3.23.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.6",
    "nodemon": "^3.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}


